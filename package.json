{
  "name": "@zeroBlue/helenjs",
  "version": "1.0.0",
  "description": "helenjs helps developers build a better accessibility experience for web apps",
  "main": "lib/index.js",
  "bin": {
    "@zeroBlue/helenjs": "lib/index.js",
    "helen": "lib/index.ts"
  },
  "config": {
    "unsafe-perm": true
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "test": "jest --config jestconfig.json",
    "build": "tsc -p .",
    "refresh": "rm -rf ./node_modules .package-lock.json && npm install",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohsaeeed/helenjs.git"
  },
  "keywords": [
    "cli",
    "helen",
    "JavaScript",
    "Accessibility",
    "Speech",
    "ChatBot"
  ],
  "author": "zeroBlue",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohsaeeed/helenjs/issues"
  },
  "homepage": "https://github.com/mohsaeeed/helenjs#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/ora": "^3.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/yargs": "^13.0.2",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^3.0.0",
    "core-js": "^3.2.1",
    "figlet": "^1.2.3",
    "inquirer": "^6.5.1",
    "ora": "^3.4.0",
    "path": "^0.12.7",
    "yargs": "^14.0.0"
  }
}
